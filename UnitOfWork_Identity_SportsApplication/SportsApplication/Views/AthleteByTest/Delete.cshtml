@model SportsApplication.Data.Entity.DeleteAthleteDataModel

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Delete Athlete Data</h3>
<hr />
<h3>Are you sure you want to delete this data?</h3>
<div>
    <form asp-action="Delete">
        <input type="hidden" asp-for="@Model.testId" />
        <input type="hidden" asp-for="@Model.id" />
        <input type="submit" value="Delete" asp-action="DeleteConfirmed" class="btn btn-danger" /> |
        <a asp-action="Index" asp-route-id="@Model.testId">Back to List</a>
    </form>
</div>
