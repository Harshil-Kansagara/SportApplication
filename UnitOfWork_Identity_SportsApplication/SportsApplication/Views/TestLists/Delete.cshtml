@model SportsApplication.Data.Entity.TestList

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Delete TestList</h3>
<hr />
<h3>Are you sure you want to delete this?</h3>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            <b>Date:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.date)
        </dd>
        <dt class="col-sm-2">
            <b>Participant Number:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.participant_num)
        </dd>
        <dt class="col-sm-2">
            <b>Test Type:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.test_type)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
